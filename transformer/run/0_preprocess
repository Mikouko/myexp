python preprocess.py -train_src data/src-train_word.txt -train_tgt data/tgt-train_word.txt -valid_src data/src-val_word.txt -valid_tgt data/tgt-val_word.txt -save_data data/data -src_seq_length 100 -tgt_seq_length 25 -dynamic_dict -share_vocab



